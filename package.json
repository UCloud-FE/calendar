{
  "name": "@z-r/calendar",
  "version": "0.0.3",
  "description": "A easy to use calendar",
  "main": "lib/index.js",
  "files": [
    "lib",
    "build",
    "types"
  ],
  "typings": "types/index.d.ts",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "bootstrap": "yarn || npm i",
    "start": "webpack serve --config sample/webpack.config.js",
    "build": "rm -rf build/ && NODE_ENV=production webpack",
    "build:lib": "rm -rf lib/ && babel src/ --extensions '.js,.jsx,.ts,.tsx' -d lib/",
    "build:ts": "rm -rf types/ && tsc",
    "build:all": "npm run build && npm run build:lib && npm run build:ts",
    "before-publish": "npm run build:all",
    "publish:patch": "npm run before-publish && npm version patch && npm publish",
    "publish:minor": "npm run before-publish && npm version minor && npm publish",
    "publish:major": "npm run before-publish && npm version major && npm publish"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/z-react-components/calendar.git"
  },
  "author": "ZxBing0066",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/z-react-components/calendar/issues"
  },
  "homepage": "https://github.com/z-react-components/calendar#readme",
  "devDependencies": {
    "@babel/cli": "^7.13.10",
    "@babel/core": "^7.13.10",
    "@babel/plugin-transform-runtime": "^7.13.10",
    "@babel/preset-env": "^7.13.10",
    "@babel/preset-react": "^7.12.13",
    "@babel/preset-typescript": "^7.13.0",
    "@types/react": "^17.0.3",
    "@types/react-dom": "^17.0.2",
    "@typescript-eslint/eslint-plugin": "^4.18.0",
    "@typescript-eslint/parser": "^4.18.0",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.2.2",
    "css-loader": "^5.1.3",
    "eslint": "^7.21.0",
    "eslint-plugin-flowtype": "^5.4.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "html-webpack-plugin": "^5.3.0",
    "moment": "^2.29.1",
    "prettier": "^2.2.1",
    "react": "^16",
    "react-dom": "^16",
    "sass": "^1.32.8",
    "sass-loader": "^11.0.1",
    "scss-loader": "^0.0.1",
    "style-loader": "^2.0.0",
    "typescript": "^4.2.3",
    "webpack": "^5.24.4",
    "webpack-bundle-analyzer": "^4.4.0",
    "webpack-cli": "^4.5.0",
    "webpack-dev-server": "^3.11.2"
  },
  "peerDependencies": {
    "react": "^16",
    "react-dom": "^16"
  },
  "dependencies": {
    "@babel/runtime": "^7.13.10",
    "core-js": "^3.9.1",
    "dayjs": "^1.10.4"
  }
}
